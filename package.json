{
  "name": "flexidropdown",
  "version": "0.1.13",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/catesson/flexidropdown"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4",
    "react-test-renderer": "^16.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "sass": "sass --watch ./src/lib/styles/styles.scss:./src/lib/styles/styles.css",
    "build-examples": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "node-sass": "^9.0.0",
    "styled-components": "^5.3.11"
  }
}
